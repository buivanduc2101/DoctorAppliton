    <fragment
        android:id="@+id/navigation_covid"
        android:name="com.sh.doctorapplication.ui.covid.CovidFragment"
        android:label="Covid-19"
        tools:layout="@layout/fragment_covid" />

    <fragment
        android:id="@+id/navigation_timkiem"
        android:name="com.sh.doctorapplication.ui.timkiem.TimKiemFragment"
        android:label="Tìm kiếm"
        tools:layout="@layout/fragment_tim_kiem" />

    <fragment
        android:id="@+id/navigation_caidat"
