<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_nhombenh">

    <fragment
        android:id="@+id/navigation_nhombenh"
        android:name="com.sh.doctorapplication.ui.nhombenh.NhomBenhFragment"
        android:label="Nhóm bệnh"
        tools:layout="@layout/fragment_nhom_benh" />

    <fragment
        android:id="@+id/navigation_benhvien"
        android:name="com.sh.doctorapplication.ui.benhvien.BenhVienFragment"
        android:label="Bệnh viện"
